{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "openocd",
            "cwd": "${workspaceRoot}",
            "executable": "target/thumbv7em-none-eabihf/debug/examples/blinky",
            "name": "Blinky",
            "device": "ATSAM4E16E",
            "runToMain": true,
            "svdFile": "${workspaceRoot}/.vscode/ATSAM4E16E.svd",
            "configFiles": [
                "./openocd.cfg"
            ],
            "preLaunchCommands": [
                "set remotetimeout 5",
                "set print asm-demangle on",
                "set backtrace limit 32",
                "set output-radix 16",
                
                "break DefaultHandler",
                "break HardFault",
                "break rust_begin_unwind",

                "monitor arm semihosting enable"
            ]
        },
        {
            "type": "cortex-debug",
