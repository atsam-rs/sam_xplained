{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build --examples",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "rust: cargo build examples"
		},
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo build library only"
		},
		{
			"type": "cargo",
			"command": "clean",
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo clean"
		},
	]
}